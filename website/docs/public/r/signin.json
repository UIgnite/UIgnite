{"name":"signin","type":"registry:ui","dependencies":[],"registryDependencies":["https://080a-104-28-199-189.ngrok-free.app/r/signin.json"],"files":[{"path":"registry/default/ui/SignIn.tsx","type":"registry:ui","content":"import React from 'react';\nimport {Input} from '@/components/Input';\nimport {\n  Card,\n  CardContent,\n  CardDescription,\n  CardFooter,\n  CardHeader,\n  CardTitle,\n} from '@/components/Card';\nimport {Button} from '@/components/Button';\n\nconst SignIn: React.FC = () => {\n  return (\n    <Card className=\"w-[420px] py-6 px-10 text-zinc-800 dark:text-zinc-200\">\n      <CardHeader>\n        <CardTitle className=\"text-4xl\">Sign In</CardTitle>\n        <CardDescription className=\"text-base font-normal\">\n          Welcome back! Please sign in.\n        </CardDescription>\n      </CardHeader>\n\n      <CardContent>\n        <form className=\"mt-4\">\n          <div className=\"grid w-full items-center gap-4\">\n            <div className=\"flex flex-col space-y-1.5\">\n              <label htmlFor=\"email\" className=\"font-medium\">\n                Email\n              </label>\n              <Input id=\"email\" placeholder=\"Enter your email\" type=\"email\" />\n            </div>\n\n            <div className=\"flex flex-col space-y-1.5\">\n              <label htmlFor=\"password\" className=\"font-medium\">\n                Password\n              </label>\n              <Input\n                id=\"password\"\n                placeholder=\"Enter your password\"\n                type=\"password\"\n              />\n            </div>\n\n            <div className=\"flex items-center justify-between\">\n              <div className=\"flex items-center space-x-2\">\n                <input\n                  id=\"remember\"\n                  type=\"checkbox\"\n                  className=\"h-4 w-4 rounded border border-gray-300\"\n                />\n                <label htmlFor=\"remember\">Remember me</label>\n              </div>\n              <a href=\"#\" className=\"text-sm text-blue-500 hover:underline\">\n                Forgot password?\n              </a>\n            </div>\n          </div>\n        </form>\n      </CardContent>\n\n      <CardFooter className=\"flex flex-col space-y-2\">\n        <Button className=\"w-full text-base\">Sign In</Button>\n        <p className=\"text-sm text-center\">\n          Not registered?{' '}\n          <a href=\"#\" className=\"text-blue-500 hover:underline\">\n            Create an account\n          </a>\n        </p>\n      </CardFooter>\n    </Card>\n  );\n};\n\nexport default SignIn;\n"}],"$schema":"https://ui.shadcn.com/schema/registry-item.json"}