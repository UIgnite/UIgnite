      "content": "// src/components/SignUpCard.tsx\nimport React from 'react';\nimport {Input} from '@/components/Input';\nimport {\n  Card,\n  CardContent,\n  CardDescription,\n  CardFooter,\n  CardHeader,\n  CardTitle,\n} from '@/components/Card';\nimport {Button} from '@/components/Button';\n\nconst SignUp: React.FC = () => {\n  return (\n    <Card className=\"w-[420px] py-6 px-10 text-zinc-800 dark:text-zinc-200\">\n      <CardHeader>\n        <CardTitle className=\"text-4xl\">Sign Up</CardTitle>\n        <CardDescription className=\"text-base font-normal\">\n          Create a new account to get started.\n        </CardDescription>\n      </CardHeader>\n\n      <CardContent>\n        <form className=\"mt-4\">\n          <div className=\"grid w-full items-center gap-4\">\n            <div className=\"flex flex-col space-y-1.5\">\n              <label htmlFor=\"name\" className=\"font-medium\">\n                Name\n              </label>\n              <Input id=\"name\" placeholder=\"Enter your name\" />\n            </div>\n\n            <div className=\"flex flex-col space-y-1.5\">\n              <label htmlFor=\"email\" className=\"font-medium\">\n                Email\n              </label>\n              <Input id=\"email\" type=\"email\" placeholder=\"Enter your email\" />\n            </div>\n\n            <div className=\"flex flex-col space-y-1.5\">\n              <label htmlFor=\"password\" className=\"font-medium\">\n                Password\n              </label>\n              <Input\n                id=\"password\"\n                type=\"password\"\n                placeholder=\"Choose a password\"\n              />\n            </div>\n          </div>\n        </form>\n      </CardContent>\n\n      <CardFooter className=\"flex flex-col gap-2 mt-8\">\n        <Button className=\"w-full text-base\">Create Account</Button>\n        <p className=\"text-sm text-center\">\n          Already have an account?{' '}\n          <a href=\"#\" className=\"text-blue-500 hover:underline\">\n            Login here\n          </a>\n        </p>\n      </CardFooter>\n    </Card>\n  );\n};\n\nexport {SignUp};\n",
